{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js",
            "continue": true
        }
    ],
    "regions": ["sin1"],
    "functions": {
        "server.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
} 