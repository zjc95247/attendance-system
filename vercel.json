{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "server.js",
            "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
        },
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "regions": ["sin1"],
    "env": {
        "NODE_ENV": "production"
    }
} 